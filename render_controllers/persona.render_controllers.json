{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.persona_animated_face.first_person": {
      "rebuild_animation_matrices": true,
      "geometry": "geometry.animated_face",
      "materials": [ { "*": "material.animated" } ],
      "textures": [ "texture.animated_face" ],
      "part_visibility": [
        { "*": false },
        { "rightArm": "q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'filled_map'" },
        { "rightSleeve": "q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'filled_map'" },
        { "leftArm": "((q.get_equipped_item_name(0, 1) == 'filled_map' && q.get_equipped_item_name(1) != 'shield') || q.get_equipped_item_name(1) == 'filled_map') && (q.get_equipped_item_name == 'bow' || q.get_equipped_item_name == 'crossbow' ? q.main_hand_item_use_duration <= 0.0f && !q.item_is_charged : 1.0)" },
        { "leftSleeve": "((q.get_equipped_item_name(0, 1) == 'filled_map' && q.get_equipped_item_name(1) != 'shield') || q.get_equipped_item_name(1) == 'filled_map') && (q.get_equipped_item_name == 'bow' || q.get_equipped_item_name == 'crossbow' ? q.main_hand_item_use_duration <= 0.0f && !q.item_is_charged : 1.0)" }
      ],
      "uv_anim": {
        "offset": [ 0.0, "v.use_blinking_animation ? v.is_blinking * 0.5 : Math.mod(Math.floor(q.life_time * 7),v.animation_frames_face)/v.animation_frames_face" ],
        "scale": [ 1.0, 1.0 ]
      },
      "is_hurt_color":{
        "r": "v.hurt_time = q.life_time; return 0.0;",
        "g": 0.0,
        "b": 0.0,
        "a": 0.0
      },
      "on_fire_color":{}
    },
