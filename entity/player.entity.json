        ],
        "pre_animation": [
          //bug: scoping spyglass in 1st person makes the new limb animation freeze, idk how to fix
          "variable.delta_time = (variable.is_first_person ? query.delta_time : query.last_frame_time(0));",
          "variable.yaw_fix = variable.yaw_fix + ((variable.is_first_person || (query.life_time <= 1) || query.is_sleeping || query.is_riding || query.is_gliding || query.is_swimming || query.can_damage_nearby_mobs) - variable.yaw_fix) * (1 - math.pow((1 - 0.8), variable.delta_time / 0.2));",
          "temp.normalized_vertical_speed = math.clamp(math.pow(math.abs(query.position_delta(1) + 0.0784), 0.225) * 1.175, 0, 0.785); variable.vertical_speed = variable.vertical_speed + (temp.normalized_vertical_speed - variable.vertical_speed) * (1 - math.pow((1 - 0.85), variable.delta_time / 0.15));",
          "variable.raw_move_speed = math.clamp(math.sqrt(query.position_delta(0) * query.position_delta(0) * (1 - variable.vertical_speed) + query.position_delta(2) * query.position_delta(2) * (1 - variable.vertical_speed)) * 1.1224341224, 0, 0.1532167643) * 6.5267009427;",
          "variable.modified_move_speed = variable.modified_move_speed + (variable.raw_move_speed - variable.modified_move_speed) * (1 - math.pow((1 - 0.8), variable.delta_time / 0.2));",
          "variable.modified_distance_moved = variable.modified_distance_moved + variable.modified_move_speed * variable.delta_time * 20;",
          "temp.r = math.round(query.armor_color_slot(0, 0) * 255); temp.g = math.round(query.armor_color_slot(0, 1) * 255); variable.leather_armor_helmet_color_index = ((temp.r == 160) ? 0 : ((temp.r == 176) ? 1 : (((temp.r == 249) && (temp.g == 128)) ? 2 : ((temp.r == 254) ? 3 : ((temp.r == 128) ? 4 : ((temp.r == 94) ? 5 : ((temp.r == 58) ? 6 : ((temp.r == 22) ? 7 : ((temp.r == 60) ? 8 : ((temp.r == 137) ? 9 : ((temp.r == 199) ? 10 : ((temp.r == 243) ? 11 : (((temp.r == 240) && (temp.g == 240)) ? 12 : ((temp.r == 157) ? 13 : ((temp.r == 71) ? 14 : ((temp.r == 29) ? 15 : ((temp.r == 131) ? 16 : 0)))))))))))))))));",
          "temp.r = math.round(query.armor_color_slot(1, 0) * 255); temp.g = math.round(query.armor_color_slot(1, 1) * 255); variable.leather_armor_chestplate_color_index = ((temp.r == 160) ? 0 : ((temp.r == 176) ? 1 : (((temp.r == 249) && (temp.g == 128)) ? 2 : ((temp.r == 254) ? 3 : ((temp.r == 128) ? 4 : ((temp.r == 94) ? 5 : ((temp.r == 58) ? 6 : ((temp.r == 22) ? 7 : ((temp.r == 60) ? 8 : ((temp.r == 137) ? 9 : ((temp.r == 199) ? 10 : ((temp.r == 243) ? 11 : (((temp.r == 240) && (temp.g == 240)) ? 12 : ((temp.r == 157) ? 13 : ((temp.r == 71) ? 14 : ((temp.r == 29) ? 15 : ((temp.r == 131) ? 16 : 0)))))))))))))))));",
          "temp.r = math.round(query.armor_color_slot(2, 0) * 255); temp.g = math.round(query.armor_color_slot(2, 1) * 255); variable.leather_armor_leggings_color_index = ((temp.r == 160) ? 0 : ((temp.r == 176) ? 1 : (((temp.r == 249) && (temp.g == 128)) ? 2 : ((temp.r == 254) ? 3 : ((temp.r == 128) ? 4 : ((temp.r == 94) ? 5 : ((temp.r == 58) ? 6 : ((temp.r == 22) ? 7 : ((temp.r == 60) ? 8 : ((temp.r == 137) ? 9 : ((temp.r == 199) ? 10 : ((temp.r == 243) ? 11 : (((temp.r == 240) && (temp.g == 240)) ? 12 : ((temp.r == 157) ? 13 : ((temp.r == 71) ? 14 : ((temp.r == 29) ? 15 : ((temp.r == 131) ? 16 : 0)))))))))))))))));",
          "temp.r = math.round(query.armor_color_slot(3, 0) * 255); temp.g = math.round(query.armor_color_slot(3, 1) * 255); variable.leather_armor_boots_color_index = ((temp.r == 160) ? 0 : ((temp.r == 176) ? 1 : (((temp.r == 249) && (temp.g == 128)) ? 2 : ((temp.r == 254) ? 3 : ((temp.r == 128) ? 4 : ((temp.r == 94) ? 5 : ((temp.r == 58) ? 6 : ((temp.r == 22) ? 7 : ((temp.r == 60) ? 8 : ((temp.r == 137) ? 9 : ((temp.r == 199) ? 10 : ((temp.r == 243) ? 11 : (((temp.r == 240) && (temp.g == 240)) ? 12 : ((temp.r == 157) ? 13 : ((temp.r == 71) ? 14 : ((temp.r == 29) ? 15 : ((temp.r == 131) ? 16 : 0)))))))))))))))));",
          "v.is_local_player = !q.is_in_ui && !v.is_paperdoll && q.is_local_player;",
          //"variable.sword_equipped = (query.get_equipped_item_name == 'netherite_sword' || query.get_equipped_item_name == 'diamond_sword' || query.get_equipped_item_name == 'golden_sword' || query.get_equipped_item_name == 'iron_sword' || query.get_equipped_item_name == 'stone_sword' || query.get_equipped_item_name == 'wooden_sword');",

          "variable.tcos0 = (math.cos((v.is_local_player ? variable.modified_distance_moved : query.modified_distance_moved) * 38.1971863421) * (v.is_local_player ? variable.modified_move_speed : query.modified_move_speed) / variable.gliding_speed_value) * 57.2957795131;",
          "variable.attack_body_rot_y = math.sin(math.sqrt(variable.attack_time) * 360) * 11.4591559026;",
          "variable.item_use_normalized = (query.main_hand_item_use_duration > 0.0f) ? (query.main_hand_item_use_duration - query.frame_alpha + 1) / query.main_hand_item_max_duration : 0.0;",
          "variable.map_angle = math.clamp(1 - variable.player_x_rotation / 45.1, 0, 1);",
          "variable.helmet_layer_visible = 1.0;",
          "variable.chest_layer_visible = 1.0;",
          "variable.leg_layer_visible = 1.0;",
          "variable.boot_layer_visible = 1.0;"
        ],
