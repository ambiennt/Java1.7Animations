        "first_person_attack_rotation_crossbow": "animation.player.first_person.attack_rotation_crossbow"
      },
      "render_controllers": [
        { "controller.render.player.first_person": "variable.is_first_person" },
        { "controller.render.player.third_person": "!variable.is_first_person && !variable.map_face_icon" },
        { "controller.render.player.map": "variable.map_face_icon" },
        { "controller.render.player.fire": "!variable.is_first_person && !variable.map_face_icon && query.is_alive && query.is_on_fire" },
        { "controller.render.player.armor.attachable_bones": "query.equipment_count > 0" },
        { "controller.render.player.armor.attachable_bones_elytra": "query.armor_texture_slot(1) == 5" },
        { "controller.render.player.armor.attachable_bones_leggings": "query.has_armor_slot(2)" },
        { "controller.render.player.armor.helmet": "!variable.is_first_person && !variable.map_face_icon && (variable.hurt_time > 0.0f) && query.has_armor_slot(0) && !query.is_invisible && !query.is_emoting" },
        { "controller.render.player.armor.chestplate": "!variable.is_first_person && !variable.map_face_icon && (variable.hurt_time > 0.0f) && query.has_armor_slot(1) && !query.is_invisible && !query.is_emoting" },
        { "controller.render.player.armor.leggings": "!variable.is_first_person && !variable.map_face_icon && (variable.hurt_time > 0.0f) && query.has_armor_slot(2) && !query.is_invisible && !query.is_emoting" },
        { "controller.render.player.armor.boots": "!variable.is_first_person && !variable.map_face_icon && (variable.hurt_time > 0.0f) && query.has_armor_slot(3) && !query.is_invisible && !query.is_emoting" }
      ],
      "enable_attachables": true
