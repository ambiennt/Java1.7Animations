{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.player.swimming": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations":[
            "swimming_other",
            "swimming_arms_easing"
          ],
          "transitions": [
            { "set_fully_swimming": "variable.swim_amount >= 1.0f" }
          ]
        },
        "set_fully_swimming": {
          "animations": [
            "swimming_other",
            "swimming_arms_full"
          ],
          "transitions": [
            { "default": "variable.swim_amount < 1.0f" }
          ]
        }
      }
    }
  }
}