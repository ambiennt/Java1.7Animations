    "controller.render.persona_animated_face.first_person": {
      "rebuild_animation_matrices": true,
      "geometry": "Geometry.animated_face",
      "materials": [ { "*": "material.default" } ],
      "textures": [ "Texture.animated_face" ],
      "part_visibility": [
        { "*": false },
        { "rightArm": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'map'" },
        { "rightSleeve": "query.get_equipped_item_name(0, 1) == '' || query.get_equipped_item_name(0, 1) == 'map'" },
        { "leftArm": "(((query.get_equipped_item_name(0, 1) == 'map' && query.get_equipped_item_name(1) != 'shield') || query.get_equipped_item_name(1) == 'map') && (query.get_equipped_item_name == 'bow' || query.get_equipped_item_name == 'crossbow' ? variable.item_use_normalized <= 0.0f && !query.item_is_charged : 1.0)) || (query.get_equipped_item_name == 'crossbow' && variable.item_use_normalized > 0.0f)" },
        { "leftSleeve": "(((query.get_equipped_item_name(0, 1) == 'map' && query.get_equipped_item_name(1) != 'shield') || query.get_equipped_item_name(1) == 'map') && (query.get_equipped_item_name == 'bow' || query.get_equipped_item_name == 'crossbow' ? variable.item_use_normalized <= 0.0f && !query.item_is_charged : 1.0)) || (query.get_equipped_item_name == 'crossbow' && variable.item_use_normalized > 0.0f)" }
      ],
      "is_hurt_color":{
        "a": "variable.hurt_time = query.life_time; return 0.0;"
      },
      "on_fire_color":{}
    },
